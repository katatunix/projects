<?xml version="1.0" ?>

<sprite>

<module isset="true" x="8" y="8" w="16" h="16" cols="4" rows="2" spacex="8" spacey="8" /><!-- Particle -->
<module isset="true" x="8" y="176" w="32" h="32" cols="4" rows="1" spacex="8" spacey="8" /><!-- Questionable Brick -->
<module isset="true" x="8" y="96" w="16" h="32" cols="4" rows="1" spacex="8" spacey="8" /><!-- Coin Spinner -->
<module isset="true" x="104" y="8" w="32" h="32" cols="2" rows="1" spacex="8" spacey="8" /><!-- Mushroom -->

<frames>
	<!-- Particle -->
	<frame isset="true" count="8" moduleIndexStart="0" moduleIndexStep="1"
				xInFrameStart="0" yInFrameStart="0" xInFrameStep="0" yInFrameStep="0">
	</frame>
	
	<!-- Questionable Brick -->
	<frame isset="true" count="4" moduleIndexStart="8" moduleIndexStep="1"
				xInFrameStart="0" yInFrameStart="0" xInFrameStep="0" yInFrameStep="0">
	</frame>
	
	<!-- Coin Spinner -->
	<frame isset="true" count="4" moduleIndexStart="12" moduleIndexStep="1"
				xInFrameStart="0" yInFrameStart="0" xInFrameStep="0" yInFrameStep="0">
	</frame>
	
	<!-- Mushroom -->
	<frame isset="true" count="2" moduleIndexStart="16" moduleIndexStep="1"
				xInFrameStart="0" yInFrameStart="0" xInFrameStep="0" yInFrameStep="0">
	</frame>
	
	<map_frame index="0" name="RED_PARTICLE_LEFT" />
	<map_frame index="1" name="RED_PARTICLE_RIGHT" />
	
	<map_frame index="4" name="BLUE_PARTICLE_LEFT" />
	<map_frame index="5" name="BLUE_PARTICLE_RIGHT" />
	
	<map_frame index="16" name="MUSHROOM_RED" />
	<map_frame index="17" name="MUSHROOM_GREEN" />
</frames>

<anims>
	<anim name="QUESTION_BRICK_BLINK" loopType="CIRCULAR" delayNumber="4">
		<aframe isset="true" count="4" frameIndexStart="8" frameIndexStep="1" xInAnimStart="0" yInAnimStart="0" xInAnimStep="0" yInAnimStep="0" />
	</anim>
	
	<anim name="COIN_SPINNER" loopType="CIRCULAR" delayNumber="1">
		<aframe isset="true" count="4" frameIndexStart="12" frameIndexStep="1" xInAnimStart="-8" yInAnimStart="0" xInAnimStep="0" yInAnimStep="0" />
	</anim>
</anims>

</sprite>

